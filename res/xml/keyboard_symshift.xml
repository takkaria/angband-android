<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="@dimen/key_gap"
    android:verticalGap="@dimen/key_gap"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="17" android:keyLabel="^Q" android:keyEdgeFlags="left"/>
        <Key android:codes="23" android:keyLabel="^W"/>
        <Key android:codes="5" android:keyLabel="^E"/>
        <Key android:codes="18" android:keyLabel="^R"/>
        <Key android:codes="20" android:keyLabel="^T"/>
        <Key android:codes="25" android:keyLabel="^Y"/>
        <Key android:codes="21" android:keyLabel="^U"/>
        <Key android:codes="9" android:keyLabel="^I"/>
        <Key android:codes="15" android:keyLabel="^O"/>
        <Key android:codes="16" android:keyLabel="^P" android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="1" android:keyLabel="^A"
                android:horizontalGap="5%p" />
        <Key android:codes="19" android:keyLabel="^S"/>
        <Key android:codes="4" android:keyLabel="^D"/>
        <Key android:codes="6" android:keyLabel="^F"/>
        <Key android:codes="7" android:keyLabel="^G"/>
        <Key android:codes="8" android:keyLabel="^H"/>
        <Key android:codes="10" android:keyLabel="^J"/>
        <Key android:codes="11" android:keyLabel="^K"/>
        <Key android:codes="12" android:keyLabel="^L"/>
    </Row>

    <Row>
        <Key android:codes="26" android:keyLabel="^Z"
                android:horizontalGap="10%p" />
        <Key android:codes="24" android:keyLabel="^X"/>
        <Key android:codes="3" android:keyLabel="^C"/>
        <Key android:codes="22" android:keyLabel="^V"/>
        <Key android:codes="2" android:keyLabel="^B"/>
        <Key android:codes="14" android:keyLabel="^N"/>
        <Key android:codes="13" android:keyLabel="^M"/>
        <Key android:codes="63" android:keyLabel="\?"/>
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-6" android:keyLabel="Ctrl^"
                android:keyWidth="20%p" android:keyEdgeFlags="left"/>
        <Key android:codes="-2" android:keyLabel="123"
                android:keyWidth="20%p"/>

        <Key android:codes="57344" android:keyLabel="Esc"
                android:horizontalGap="20%p"
                android:keyWidth="20%p" />
        <Key android:codes="94" android:keyLabel="^"
                android:keyWidth="20%p" android:keyEdgeFlags="right" />
    </Row>
</Keyboard>
